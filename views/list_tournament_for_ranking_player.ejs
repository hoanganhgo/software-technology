<!DOCTYPE html>
<html>

<head>
    <title><%= title%></title>
    <% include elements/head_tag.html%>
</head>

<body onload="loadUserName()">
<%include elements/header.ejs%>
<div class="container">
    <div class="row">
        <div class="col-lg-3">
        </div>
        <div class="col-lg-7">
            <div class="row justify-content-center">
                <h2>DANH SÁCH CÁC GIẢI ĐẤU</h2>
            </div>
            <table class="table">
                <thead class="thead-light">
                <tr>
                    <th scope="col">STT</th>
                    <th scope="col">Tên giải đấu</th>
                    <th scope="col">Vua phá lưới</th>
                </tr>
                </thead>
                <tbody>
                <%listTournament.forEach(function (tournament) {%>
                <tr>
                    <th scope="row"><%=tournament.stt%></th>
                    <td><%=tournament.nameTournament%></td>
                    <td><a href="<%=tournament.urlRanking%>"><i>danh sách vua phá lưới</i></a></td>
                </tr>
                <%})%>

                </tbody>
            </table>
        </div>
    </div>
</div>

<%include elements/footer.html%>
</body>

</html>